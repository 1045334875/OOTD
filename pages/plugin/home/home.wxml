<scroll-view scroll-y class="scrollPage">
  <view class="cu-tabbar-height"></view>
  <view class="cu-card">
    <scroll-view scroll-y  class="scrollPage" style="background-image: url('/images/back2.png'); height: 170px;"></scroll-view>
<!--     <image src="/images/back2.png"  class='png' style='width:100%;height:200rpx'></image> -->
    <view class="cu-item shadow-blur"  bindtap="toChild" data-url="{{item.url}}" wx:for="{{list}}" wx:key>
      <view class="cardTitle bg-pink">
        {{item.title}}
      </view>
    </view>
  </view>
  <view class="cu-tabbar-height"></view>
</scroll-view>